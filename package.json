{
  "name": "learn-online",
  "version": "1.0.0",
  "description": "learn online - graduation thesis",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel ./src -d ./build/src",
    "build": "npm run clean && npm run build-babel",
    "production": "npm run build && node ./build/src/index.js",
    "dev": "nodemon --exec ./node_modules/.bin/babel-node ./src/index.js",
    "start": "node --experimental-modules --es-module-specifier-resolution=node src/index.js",
    "eslint": "eslint src"
  },
  "author": "lam nguyen",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.15.4",
    "axios": "^0.22.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5", 
    "express": "^4.17.1",
    "express-fileupload": "^1.3.1",
    "form-data": "^4.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mongoose": "^6.0.8",
    "socket.io": "^4.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.5",
    "@babel/eslint-parser": "^7.15.7",
    "@babel/node": "^7.15.4",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.6",
    "babel-plugin-module-resolver": "^4.1.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "nodemon": "^2.0.13"
  }
}
